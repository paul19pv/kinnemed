@model kinnemed05.Models.empresa

@{
    ViewBag.Title = "Create";
}


<div class="bs-component col-lg-12">
    <div class="panel panel-default">
        <div class="panel-heading">
            <h3 class="panel-title">Crear Empresa</h3>
        </div>
        <div class="panel-body">
            @using (Html.BeginForm())
            {
                @Html.AntiForgeryToken()
                @Html.ValidationSummary(true)
                <div class="col-lg-5">
                    <div class="form-group">
                        @Html.LabelFor(model => model.emp_cedula, new { @class = "control-label", @for = "inputDefault" })
                        @Html.TextBoxFor(model => model.emp_cedula, new { @class = "form-control" })
                        @Html.ValidationMessageFor(model => model.emp_cedula)
                    </div>
                    <div class="form-group">
                        @Html.LabelFor(model => model.emp_nombre, new { @class = "control-label", @for = "inputDefault" })
                        @Html.TextBoxFor(model => model.emp_nombre, new { @class = "form-control" })
                        @Html.ValidationMessageFor(model => model.emp_nombre)

                    </div>
                    <div class="form-group">
                        @Html.LabelFor(model => model.emp_direccion, new { @class = "control-label", @for = "inputDefault" })
                        @Html.TextBoxFor(model => model.emp_direccion, new { @class = "form-control" })
                        @Html.ValidationMessageFor(model => model.emp_direccion)
                    </div>
                    <div class="form-group">
                        @Html.LabelFor(model => model.emp_telefono, new { @class = "control-label", @for = "inputDefault" })
                        @Html.TextBoxFor(model => model.emp_telefono, new { @class = "form-control" })
                        @Html.ValidationMessageFor(model => model.emp_telefono)
                    </div>
                    <div class="form-group">
                        @Html.LabelFor(model => model.emp_mail, new { @class = "control-label", @for = "inputDefault" })
                        @Html.TextBoxFor(model => model.emp_mail, new { @class = "form-control" })
                        @Html.ValidationMessageFor(model => model.emp_mail)
                    </div>
                    <div class="form-group">
                        @Html.TextBoxFor(model => model.emp_estado, new { @class = "form-control", Value = "activo", @type = "hidden" })
                    </div>
                    <div class="form-group">
                        @Html.ActionLink("Regresar", "Index", "Empresa", new { @class = "btn btn-default" })
                        <input type="submit" value="Crear" class="btn btn-primary" />
                    </div>

                </div>
            }

        </div>
    </div>

</div>



@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
