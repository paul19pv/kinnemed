@model kinnemed05.Models.audiometria

@{
    ViewBag.Title = "Delete";
}
<div class="col-lg-12">
    <div class="panel panel-default">
        <div class="panel-heading">
            <h3 class="panel-title">Detalle Exámen Audiometría</h3>
        </div>
        <div class="panel-body">


            <div class="col-lg-4">
                <div class="form-group">
                    @Html.LabelFor(model => model.aud_paciente, new { @class = "control-label", @for = "inputDefault" })
                    @Html.TextBox("txt_paciente", "", new { @class = "form-control", Value = @ViewBag.paciente, @readonly = "readonly" })
                </div>
            </div>
            <div class="col-lg-4">
                <div class="form-group">
                    @Html.LabelFor(model => model.aud_medico, new { @class = "control-label", @for = "inputDefault" })
                    @Html.TextBox("txt_paciente", "", new { @class = "form-control", Value = @ViewBag.medico, @readonly = "readonly" })
                </div>
            </div>
            <div class="col-lg-4">
                <div class="form-group">
                    @Html.LabelFor(model => model.aud_archivo, new { @class = "control-label", @for = "inputDefault" })
                    @Html.TextBoxFor(model => model.aud_archivo, new { @class = "form-control", @readonly = "readonly" })
                </div>
            </div>
            
            <div class="col-lg-12" style="clear:both">
                <div class="form-group">
                    @Html.LabelFor(model => model.aud_observacion, new { @class = "control-label", @for = "inputDefault" })
                    @Html.TextAreaFor(model => model.aud_observacion, new { @class = "form-control", @rows = "3", @readonly = "readonly" })
                </div>
            </div>
            @using (Html.BeginForm())
            {
                @Html.AntiForgeryToken()
                <div class="col-lg-4" style="clear:both">
                    <div class="form-group">
                        <input class="btn btn-default" type="submit" value="Eliminar" />
                        @Html.ActionLink("Regresar", "Index", "Audiometria", new { }, new { @class = "btn btn-default" })
                    </div>
                </div>
            }



        </div>
    </div>
</div>

