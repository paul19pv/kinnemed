@model kinnemed05.Models.medico

@{
    ViewBag.Title = "Details";
}

<div class="col-lg-9">
    <div class="bs-component">
        <div class="panel panel-primary">
            <div class="panel-heading">
                <h3 class="panel-title">Detalle Médico</h3>
            </div>
            <div class="panel-body">
                <div class="col-lg-5">
                    <div class="form-group">
                        @Html.LabelFor(model => model.med_cedula, new { @class = "control-label", @for = "inputDefault" })
                        @Html.TextBoxFor(model => model.med_cedula, new { @class = "form-control", @readonly = "readonly" })
                    </div>
                    <div class="form-group">
                        @Html.LabelFor(model => model.med_nombres, new { @class = "control-label", @for = "inputDefault" })
                        @Html.TextBoxFor(model => model.med_nombres, new { @class = "form-control", @readonly = "readonly" })
                    </div>
                    <div class="form-group">
                        @Html.LabelFor(model => model.med_apellidos, new { @class = "control-label", @for = "inputDefault" })
                        @Html.TextBoxFor(model => model.med_apellidos, new { @class = "form-control", @readonly = "readonly" })
                    </div>
                    <div class="form-group">
                        @Html.LabelFor(model => model.med_ci, new { @class = "control-label", @for = "inputDefault" })
                        @Html.TextBoxFor(model => model.med_ci, new { @class = "form-control", @readonly = "readonly" })
                    </div>
                    <div class="form-group">
                        @Html.LabelFor(model => model.med_codigo, new { @class = "control-label", @for = "inputDefault" })
                        @Html.TextBoxFor(model => model.med_codigo, new { @class = "form-control", @readonly = "readonly" })
                    </div>
                    <div class="form-group">
                        @Html.LabelFor(model => model.especialidad.esp_nombre, new { @class = "control-label", @for = "inputDefault" })
                        @Html.TextBoxFor(model => model.especialidad.esp_nombre, new { @class = "form-control", @readonly = "readonly" })
                    </div>
                    <div class="form-group">
                        @Html.LabelFor(model => model.med_correo, new { @class = "control-label", @for = "inputDefault" })
                        @Html.TextBoxFor(model => model.med_correo, new { @class = "form-control", @readonly = "readonly" })
                    </div>
                    <div class="form-group">
                        @Html.ActionLink("Regresar", "Index", "Medico", new { @class = "btn btn-default" })
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
