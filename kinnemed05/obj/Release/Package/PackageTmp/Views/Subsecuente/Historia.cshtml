@model IEnumerable<kinnemed05.Models.historia>

@{
    ViewBag.Title = "Index";
}
<div class="panel panel-default">
    <div class="panel-body">
        <table class="table table-striped table-hover ">
            <tr>
                <th>
                    Paciente
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.his_motivo)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.his_problema)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.his_fecha)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.his_numero)
                </th>
                <th></th>
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.paciente.pac_nombres) @Html.DisplayFor(modelItem => item.paciente.pac_apellidos)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.his_motivo)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.his_problema)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.his_fecha)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.his_numero)
                    </td>
                    <td>
                        @Html.ActionLink("Create", "Create", new { id = item.his_id }, new { @class = "iconb icon-new", @title = "Crear" })
                    </td>
                </tr>
            }

        </table>

    </div>
</div>