@model IEnumerable<kinnemed05.Models.subsecuente>

@{
    ViewBag.Title = "Index";
}

<h2>Hojas Subsecuentes</h2>

<p>
    @Html.ActionLink("Nueva Hoja", "Lista", new {}, new { @class = "btn btn-default" })
    @Html.ActionLink("Regresar", "Index", "Home", new { }, new { @class = "btn btn-default" })
</p>

<div class="panel panel-default">
    <div class="panel-body">
        <table class="table table-striped table-hover ">
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.sub_fecha)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.sub_hora)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.sub_subjetivo)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.sub_objetivo)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.sub_analisis)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.sub_plan)
                </th>
                <th></th>
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.sub_fecha)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.sub_hora)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.sub_subjetivo)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.sub_objetivo)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.sub_analisis)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.sub_plan)
                    </td>
                    <td>
                      @Html.ActionLink("Edit", "Edit", new { id = item.sub_id }, new { @class = "iconb icon-edit", @title = "Editar" })
                        @Html.ActionLink("Details", "Details", new { id = item.sub_id }, new { @class = "iconb icon-detail", @title = "Detalle" })
                        @Html.ActionLink("Delete", "Delete", new { id = item.sub_id }, new { @class = "iconb icon-delete", @title = "Borrar" })
                    </td>
                </tr>
            }

        </table>


    </div>
</div>
