@model IEnumerable<kinnemed05.Models.prueba>

@{
    ViewBag.Title = "Index";
}
@Html.ValidationSummary(true)

<div class="col-lg-6">
    <div class="panel panel-default">
        <div class="panel-body">
            @Html.ValidationMessage("error")
            <table class="table table-striped table-hover ">
                <tr>
                    
                    <th>
                        @Html.DisplayNameFor(model => model.pru_examen)
                    </th>
                    <th></th>
                </tr>

                @foreach (var item in Model)
                {
                    <tr>
                        
                        <td>
                            @Html.DisplayFor(modelItem => item.examen.exa_nombre)
                        </td>

                        <td>
                            @Ajax.ActionLink("Eliminar", "Delete", new { id = item.pru_id }, new AjaxOptions { UpdateTargetId = "div_index", LoadingElementId = "div_loading", LoadingElementDuration = 100 }, new { @class = "iconb icon-delete", @title = "Borrar" })
                        </td>
                    </tr>
                }

            </table>

        </div>
    </div>
</div>