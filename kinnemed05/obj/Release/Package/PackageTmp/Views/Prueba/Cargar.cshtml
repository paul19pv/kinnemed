@{
    ViewBag.Title = "Cargar";
}

<div class="col-lg-9">
    <div class="bs-component">
        <div class="panel panel-default">
            <div class="panel-heading">
                <h3 class="panel-title">Subir Archivo</h3>
            </div>
            <div class="panel-body">


                @using (Html.BeginForm("Cargar", "Prueba", FormMethod.Post, new { @enctype = "multipart/form-data" }))
                {
                    @Html.AntiForgeryToken()
                    @Html.ValidationSummary(true)
                    <div class="col-lg-5">
                        <div class="form-group">
                            <label for="exampleInputFile">Archivo a cargar</label>
                            <input type="file" name="FileUpload" id="exampleInputFile">
                        </div>
                        <div class="form-group">
                            <input type="submit" id="upload" value="Cargar Archivo" class="btn btn-default btn-sm" />
                            @Html.ActionLink("Regresar", "Index", "Registro", new { }, new { @class = "btn btn-default" })
                        </div>
                    </div>

                }

            </div>
        </div>
    </div>
</div>
