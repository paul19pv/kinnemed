@{
    ViewBag.Title = "Examen";
}

<p>
    @Html.ActionLink("Solicitar Exámen", "Create", "Examen","" ,new { @class = "btn btn-default" })
    @Html.ActionLink("Ingresar CSV", "Create", "Biometria", "", new { @class = "btn btn-default" })
</p>

    

<div class="panel panel-default">
    <div class="panel-heading">Exámenes de Laboratorio</div>
    <div class="panel-body">
        <a class="btn btn-default btn-lg btn-img" title="Lista General" href="@Url.Content("~/Examen/Index")">
            <img src="@Url.Content("~/Content/img/iconos/examen-icon.png")" class="img-rounded" />
            <label>Todos</label>
        </a>

        <a class="btn btn-default btn-lg btn-img" title="Biometría" href="@Url.Content("~/Examen/Index?grupo=9")">
            <img src="@Url.Content("~/Content/img/iconos/examen01-icon.png")" class="img-rounded" />
            <label>Biometría</label>
        </a>
        <a class="btn btn-default btn-lg btn-img" title="Químico Sanguineo" href="@Url.Content("~/Examen/Index?grupo=2")">
            <img src="@Url.Content("~/Content/img/iconos/examen02-icon.png")" class="img-rounded" />
            <label>Químico</label>
        </a>
    </div>
</div>
