@model IEnumerable<kinnemed05.Models.ocupacional>

@{
    ViewBag.Title = "Index";
}
<div class="col-lg-12">
    <div class="panel panel-default">
        <div class="panel-body">
            <table class="table table-striped table-hover">
                <tr>
                    <th>
                        @Html.DisplayNameFor(model => model.ocu_empresa)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.ocu_seccion)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.ocu_cargo)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.ocu_descripcion)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.ocu_jornada)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.ocu_inicio)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.ocu_fin)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.ocu_tiempo)
                    </th>
                    <th></th>
                </tr>

                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.ocu_empresa)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.ocu_seccion)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.ocu_cargo)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.ocu_descripcion)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.ocu_jornada)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.ocu_inicio)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.ocu_fin)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.ocu_tiempo)
                        </td>
                        <td>
                            @Ajax.ActionLink("Eliminar", "Delete", new { id = item.ocu_id }, new AjaxOptions { UpdateTargetId = "div_index", LoadingElementId = "div_loading", LoadingElementDuration = 100 }, new { @class = "iconb icon-delete", @title = "Borrar" })
                        </td>
                    </tr>
                }

            </table>

        </div>
        

    </div>
</div>


