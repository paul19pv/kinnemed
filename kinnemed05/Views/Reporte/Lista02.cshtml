@model IEnumerable<kinnemed05.Models.Lista02>
<label>@ViewBag.empresa</label>
<div class=" panel panel-default">
    <div class="panel-body">
        <table class="table table-striped table-hover ">
            <tr>
                
                <th>
                    @Html.DisplayNameFor(model => model.Puesto)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Tipo)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Laboratorio)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Audiometria)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Oftalmologia)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Espirometria)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Rayos)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Chequeo)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Certificado)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Diagnostico1)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Diagnostico2)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Diagnostico3)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Diagnostico4)
                </th>
                <th></th>
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    
                    <td>
                        @Html.DisplayFor(modelItem => item.Puesto)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Tipo)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Laboratorio)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Audiometria)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Oftalmologia)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Espirometria)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Rayos)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Chequeo)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Certificado)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Diagnostico1)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Diagnostico2)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Diagnostico3)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Diagnostico4)
                    </td>
                </tr>
            }

        </table>


    </div>
</div>
