@model kinnemed05.Models.trabajador

@{
    ViewBag.Title = "Details";
}
<div class="col-lg-12">
    <div class="panel panel-default">
        <div class="panel-heading">
            <h3 class="panel-title">Detalle Trabajador Ocupacional</h3>
        </div>
        <div class="panel-body">
            <div class="col-lg-4">
                <div class="form-group">
                    @Html.LabelFor(model => model.tra_cedula, new { @class = "control-label" })
                    @Html.TextBoxFor(model => model.tra_cedula, new { @class = "form-control", @readonly = "readonly" })
                </div>
                <div class="form-group">
                    @Html.LabelFor(model => model.tra_correo, new { @class = "control-label" })
                    @Html.TextBoxFor(model => model.tra_correo, new { @class = "form-control", @readonly = "readonly" })
                </div>
            </div>
            <div class="col-lg-4">
                <div class="form-group">
                    @Html.LabelFor(model => model.tra_nombres, new { @class = "control-label" })
                    @Html.TextBoxFor(model => model.tra_nombres, new { @class = "form-control", @readonly = "readonly" })
                </div>
                <div class="form-group">
                    @Html.LabelFor(model => model.tra_empresa, new { @class = "control-label" })
                    @Html.TextBoxFor(model => model.tra_empresa, new { @class = "form-control", @readonly = "readonly" })
                </div>
            </div>
            <div class="col-lg-4">
                <div class="form-group">
                    @Html.LabelFor(model => model.tra_apellidos, new { @class = "control-label" })
                    @Html.TextBoxFor(model => model.tra_apellidos, new { @class = "form-control",@readonly="readonly" })
                </div>
            </div>
            <div class="col-lg-4" style="clear:both">
                <div class="form-group">
                    @Html.ActionLink("Regresar", "Index", "Trabajador", new { }, new { @class = "btn btn-default" })
                </div>
            </div>


        </div>
    </div>
</div>


