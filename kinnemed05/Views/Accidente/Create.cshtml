@model kinnemed05.Models.accidente

@{
    ViewBag.Title = "Create";
}

<h2>Create</h2>

@using (Html.BeginForm()) {
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)

    <fieldset>
        <legend>accidente</legend>

        <div class="editor-label">
            @Html.LabelFor(model => model.acc_descripción)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.acc_descripción)
            @Html.ValidationMessageFor(model => model.acc_descripción)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.acc_capacidad)
        </div>
        <div class="editor-field">
