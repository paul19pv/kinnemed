        <div class="editor-label">
            @Html.LabelFor(model => model.acc_fecha)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.acc_fecha)
            @Html.ValidationMessageFor(model => model.acc_fecha)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.acc_empresa)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.acc_empresa)
            @Html.ValidationMessageFor(model => model.acc_empresa)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.acc_paciente, "paciente")
