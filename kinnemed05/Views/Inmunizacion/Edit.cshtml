@model kinnemed05.Models.inmunizacion

@{
    ViewBag.Title = "Edit";
}

<h2>Edit</h2>

@using (Html.BeginForm()) {
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)

    <fieldset>
        <legend>inmunizacion</legend>

        <div class="editor-label">
            @Html.LabelFor(model => model.inm_id)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.inm_id)
            @Html.ValidationMessageFor(model => model.inm_id)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.inm_vacuna, "vacuna")
        </div>
        <div class="editor-field">
            @Html.DropDownList("inm_vacuna", String.Empty)
            @Html.ValidationMessageFor(model => model.inm_vacuna)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.inm_fecha)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.inm_fecha)
            @Html.ValidationMessageFor(model => model.inm_fecha)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.inm_tipo)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.inm_tipo)
            @Html.ValidationMessageFor(model => model.inm_tipo)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.inm_paciente, "paciente")
        </div>
        <div class="editor-field">
            @Html.DropDownList("inm_paciente", String.Empty)
            @Html.ValidationMessageFor(model => model.inm_paciente)
        </div>

        <p>
            <input type="submit" value="Save" />
        </p>
    </fieldset>
}

<div>
    @Html.ActionLink("Back to List", "Index")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
