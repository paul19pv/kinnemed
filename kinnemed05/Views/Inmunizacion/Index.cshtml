@model IEnumerable<kinnemed05.Models.inmunizacion>

@{
    ViewBag.Title = "Index";
}
<div class="col-lg-12">
    <div class="panel panel-default">
        <div class="panel-body">
            <table class="table table-striped table-hover">
                <tr>
                    <th>
                        @Html.DisplayNameFor(model => model.inm_vacuna)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.inm_fecha)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.inm_tipo)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.inm_paciente)
                    </th>
                    <th></th>
                </tr>

                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.vacuna.vac_nombre)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.inm_fecha)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.inm_tipo)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.inm_paciente)
                        </td>
                        <td>
                            @Ajax.ActionLink("Eliminar", "Delete", new { id = item.inm_id }, new AjaxOptions { UpdateTargetId = "div_index", LoadingElementId = "div_loading", LoadingElementDuration = 100 }, new { @class = "iconb icon-delete", @title = "Borrar" })
                        </td>
                    </tr>
                }

            </table>

        </div>
    </div>
</div>


