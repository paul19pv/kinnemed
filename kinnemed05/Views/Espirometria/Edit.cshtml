@model kinnemed05.Models.espirometria

@{
    ViewBag.Title = "Edit";
}

<h2>Edit</h2>

@using (Html.BeginForm()) {
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)

    <fieldset>
        <legend>espirometria</legend>

        <div class="editor-label">
            @Html.LabelFor(model => model.esp_id)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.esp_id)
            @Html.ValidationMessageFor(model => model.esp_id)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.esp_paciente, "paciente")
        </div>
        <div class="editor-field">
            @Html.DropDownList("esp_paciente", String.Empty)
            @Html.ValidationMessageFor(model => model.esp_paciente)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.esp_archivo)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.esp_archivo)
            @Html.ValidationMessageFor(model => model.esp_archivo)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.esp_observacion)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.esp_observacion)
            @Html.ValidationMessageFor(model => model.esp_observacion)
        </div>

        <p>
            <input type="submit" value="Save" />
        </p>
    </fieldset>
}

<div>
    @Html.ActionLink("Back to List", "Index")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
