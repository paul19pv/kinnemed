@model kinnemed05.Models.examen

@{
    ViewBag.Title = "Delete";
}

<div class="col-lg-12">
    <div class="panel panel-default">
        <div class="panel panel-heading">
            <h3 class="panel-title">Detalle Prueba Laboratorio</h3>
        </div>
        <div class="panel panel-body">
            <div class="col-lg-4">
                <div class="form-group">
                    @Html.LabelFor(model => model.exa_nombre, new { @class = "control-label", @for = "inputDefault" })
                    @Html.TextBoxFor(model => model.exa_nombre, new { @class = "form-control", @readonly = "readonly" })
                </div>
                <div class="form-group">
                    @Html.LabelFor(model => model.exa_unidad, new { @class = "control-label", @for = "inputDefault" })
                    @Html.TextBoxFor(model => model.exa_unidad, new { @class = "form-control", @readonly = "readonly" })
                </div>
            </div>
            <div class="col-lg-4">
                <div class="form-group">
                    @Html.LabelFor(model => model.exa_area, new { @class = "control-label", @for = "inputDefault" })
                    @Html.TextBoxFor(model => model.area.are_nombre, new { @class = "form-control", @readonly = "readonly" })
                </div>
                <div class="form-group">
                    @Html.LabelFor(model => model.exa_orden, new { @class = "control-label", @for = "inputDefault" })
                    @Html.TextBoxFor(model => model.exa_nombre, new { @class = "form-control", @readonly = "readonly" })
                </div>
            </div>
            <div class="col-lg-4">
                <div class="form-group">
                    @Html.LabelFor(model => model.exa_valores, new { @class = "control-label", @for = "inputDefault" })
                    @Html.TextAreaFor(model => model.exa_valores, new { @class = "form-control", @rows = "3", @readonly = "readonly" })
                </div>
            </div>
            @using (Html.BeginForm())
            {
                @Html.AntiForgeryToken()
                <div class="col-lg-4" style="clear:both">
                    <div class="form-group">
                        <input class="btn btn-default" type="submit" value="Eliminar" />
                        @Html.ActionLink("Regresar", "Index", "Espirometria", new { }, new { @class = "btn btn-default" })
                    </div>
                </div>
            }

        </div>
    </div>
</div>