@model kinnemed05.Models.rayos

@{
    ViewBag.Title = "Details";
}

<div class="col-lg-12">
    <div class="panel panel-default">
        <div class="panel-heading">
            <h3 class="panel-title">Detalle Exámen Rayos X</h3>
        </div>
        <div class="panel-body">


            <div class="col-lg-4">
                <div class="form-group">
                    @Html.LabelFor(model => model.ray_paciente, new { @class = "control-label", @for = "inputDefault" })
                    @Html.TextBox("txt_paciente", "", new { @class = "form-control", Value = @ViewBag.paciente, @readonly = "readonly" })
                </div>
            </div>
            
            <div class="col-lg-4">
                <div class="form-group">
                    @Html.LabelFor(model => model.ray_imagen, new { @class = "control-label", @for = "inputDefault" })
                    @Html.TextBoxFor(model => model.ray_imagen, new { @class = "form-control", @readonly = "readonly" })
                </div>
            </div>
            <div class="col-lg-12" style="clear:both">
                <div class="form-group">
                    @Html.LabelFor(model => model.ray_observacion, new { @class = "control-label", @for = "inputDefault" })
                    @Html.TextAreaFor(model => model.ray_observacion, new { @class = "form-control", @rows = "3", @readonly = "readonly" })
                </div>
            </div>

            <div class="col-lg-4" style="clear:both">
                <div class="form-group">
                    @Html.ActionLink("Regresar", "Index", "Rayos", new { }, new { @class = "btn btn-default" })
                </div>
            </div>


        </div>
    </div>
</div>
